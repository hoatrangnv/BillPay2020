package com.viettel.paybonus.obj;

import java.math.BigDecimal;

/**
 *
 * @author dev_bacnx
 */
public class QueryInforResponse {

	public String ResponseCode;
	public String ResponseMessage;
	public String Status;
	public BigDecimal Balance;
	public String CommisionBlance;
	public String ChannelCode;
	public String ProviderCode;
	public String CustomerCode;

	public String getResponseCode() {
		return ResponseCode;
	}

	public void setResponseCode(String ResponseCode) {
		this.ResponseCode = ResponseCode;
	}

	public String getResponseMessage() {
		return ResponseMessage;
	}

	public void setResponseMessage(String ResponseMessage) {
		this.ResponseMessage = ResponseMessage;
	}

	public String getStatus() {
		return Status;
	}

	public void setStatus(String Status) {
		this.Status = Status;
	}

	public BigDecimal getBalance() {
		return Balance;
	}

	public void setBalance(BigDecimal Balance) {
		this.Balance = Balance;
	}

	public String getCommisionBlance() {
		return CommisionBlance;
	}

	public void setCommisionBlance(String CommisionBlance) {
		this.CommisionBlance = CommisionBlance;
	}

	public String getChannelCode() {
		return ChannelCode;
	}

	public void setChannelCode(String ChannelCode) {
		this.ChannelCode = ChannelCode;
	}

	public String getProviderCode() {
		return ProviderCode;
	}

	public void setProviderCode(String ProviderCode) {
		this.ProviderCode = ProviderCode;
	}

	public String getCustomerCode() {
		return CustomerCode;
	}

	public void setCustomerCode(String CustomerCode) {
		this.CustomerCode = CustomerCode;
	}

	@Override
	public String toString() {
		return "QueryInforResponse{" + "ResponseCode=" + ResponseCode + ", ResponseMessage=" + ResponseMessage + ", Status=" + Status + ", Balance=" + Balance + ", CommisionBlance=" + CommisionBlance + ", ChannelCode=" + ChannelCode + ", ProviderCode=" + ProviderCode + ", CustomerCode=" + CustomerCode + '}';
	}




}
