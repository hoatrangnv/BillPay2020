package com.viettel.vas.wsfw.object;

/**
 *
 * @author kdvt_minhnh@viettel.com.vn
 * @since Dec 21, 2011
 * @version 1.0
 */
public class UserInfo {

    public static final String ID = "WS_USER_ID";
    public static final String USERNAME = "USERNAME";
    public static final String PASSWORD = "PASSWORD";
    public static final String IP_ADDRESS = "IP_ADDRESS";
    public static final String CHANNEL = "CHANNEL";
    //
    public static final long USER_WRONG = -1;
    public static final long NOT_ALLOW = -2;
//    public static final long WRONG_CHANNEL = -3;
    public static final long WRONG_MSISDN = -3;
    public static final long FAIL = 1;
    private long id;
    private String user;
    private String password;
    private String ip;
    private String channel;
    private String partnerCode;
    private String fundCode;

    public String getChannel() {
        return channel;
    }

    public void setChannel(String channel) {
        this.channel = channel;
    }

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public String getIp() {
        return ip;
    }

    public void setIp(String ip) {
        this.ip = ip;
    }

    public String getPassword() {
        return password;
    }

    public void setPassword(String password) {
        this.password = password;
    }

    public String getUser() {
        return user;
    }

    public void setUser(String user) {
        this.user = user;
    }

    public String getPartnerCode() {
        return partnerCode;
    }

    public void setPartnerCode(String partnerCode) {
        this.partnerCode = partnerCode;
    }

    public String getFundCode() {
        return fundCode;
    }

    public void setFundCode(String fundCode) {
        this.fundCode = fundCode;
    }
}
